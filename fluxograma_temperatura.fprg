    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Temperatura" type="Integer" array="False" size=""/>
            <output expression="&quot;Digite o valor da temperatura: &quot;" newline="True"/>
            <input variable="Temperatura"/>
            <if expression="Temperatura &lt; 0">
                <then>
                    <output expression="&quot;Valor de temperatura inv&#225;lido.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="Temperatura &gt; 50 &amp;&amp; Temperatura &lt; 100">
