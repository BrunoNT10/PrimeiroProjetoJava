                            <output expression="&quot;Temperatura do GPU elevada. &quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="Temperatura &gt; 100">
                                <then>
                                    <output expression="&quot;CUIDADO! Temperatura do GPU acima do permitido. &quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Temperatura da GPU normal.&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
